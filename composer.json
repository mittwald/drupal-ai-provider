{
  "name": "mittwald/drupal-ai-provider",
  "description": "mittwald provider implementation for the AI module for Drupal",
  "type": "drupal-module",
  "license": "GPL-2.0-or-later",
  "require": {
    "drupal/ai": "^1.2.0",
    "drupal/key": "^1.18"
  },
  "repositories": {
    "drupal": {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    }
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": false
    }
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1"
  }
}
